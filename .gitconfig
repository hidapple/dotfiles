[core]
  excludesfile = ~/.gitignore
  editor = vim
  whitespace = cr-at-eol
[alias]
  graph = log --graph --date-order --all --pretty=format:'%h %Cred%d %Cgreen%ad %Cblue%cn %Creset%s' --date=short
  st    = status
  br    = branch
  co    = checkout
[push]
  default = simple
[include]
  path = ~/.gitconfig.local
